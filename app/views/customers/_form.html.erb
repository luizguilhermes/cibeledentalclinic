<%= simple_form_for @customer do |f| %>
	<%= f.input :customer_name, label: 'Nome do paciente', class: 'form-control' %>
	<%= f.input :customer_cpf, label: 'CPF' %>
	<%= f.input :customer_rg, label: 'RG' %>
	<%= f.input :customer_gender, label: 'Sexo' %>
	<br>
	<%= link_to 'Voltar', root_path, class: 'btn btn-primary' %>
	<%= f.button :submit, submit_text, class: 'btn btn-primary' %>
<% end %>